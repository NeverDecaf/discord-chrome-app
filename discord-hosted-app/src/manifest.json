{
"manifest_version": 2,
"name": "Discord",
"description": "Discord as hosted app",
"version": "1.0",
"icons": {
"128": "128.png"
},
"app": {
"urls": [
"https://discordapp.com/"
],
"launch": {
"web_url": "https://discordapp.com/channels/@me",
"container":"panel"
}
},
"permissions": [
"unlimitedStorage",
"notifications"
]
}